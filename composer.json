{
    "name": "joshluongo/laravel-aus",
    "description": "Support for down under.",
    "keywords": [
        "joshluongo",
        "laravel",
        "laravel-aus"
    ],
    "homepage": "https://github.com/joshluongo/laravel-aus",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Luongo",
            "email": "josh@luongo.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "nunomaduro/collision": "^7.8",
        "nunomaduro/larastan": "^2.0.1",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelAus\\LaravelAus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAus\\LaravelAus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAus\\LaravelAus\\LaravelAusServiceProvider"
            ],
            "aliases": {
                "LaravelAus": "LaravelAus\\LaravelAus\\Facades\\LaravelAus"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
